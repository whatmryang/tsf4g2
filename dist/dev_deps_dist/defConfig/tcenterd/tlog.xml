<TLOGConf>
	<Priority>DEBUG</Priority>
	<DelayInit>0</DelayInit>
	<SuppressError>1</SuppressError>
<CategoryList>
	<Name>tlog.centerd</Name>
	<Priority>TRACE</Priority>
	<Additive>1</Additive>
	<Format>[%d] %m</Format>
	<Filter>
		<Count>1</Count>
		<Filters>
			<IDFilter>
				<Start>81206821</Start>
				<Count>100</Count>
			</IDFilter>			
		</Filters>
	</Filter>
	<Device>
		<Type>FILE</Type>
		<Device>			
			<File>
				<MaxRotate>3</MaxRotate>
				<SizeLimit>10000000</SizeLimit>
				<SyncTime>0</SyncTime>
				<Precision>1</Precision>
				<Pattern>../log/centerd.log</Pattern>					
			</File>					
		</Device>
	</Device>
</CategoryList>
</TLOGConf>
