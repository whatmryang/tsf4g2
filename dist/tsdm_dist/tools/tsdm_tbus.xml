<?xml version="1.0" encoding="gbk" standalone="yes" ?>
<!-- This file is for tbus routes tool application-->

<TbusGCIM>

  <!-- tagent系统分配的业务ID，不使用tagent系统配置缺省值0  -->
  <BussinessID>0</BussinessID>
   
  <!-- 十进制点分表示法表示的通信地址模版  -->
  <AddrTemplet>8.8.8.8</AddrTemplet>
   
   <!-- GCIM共享内存key -->
  <GCIMShmKey>1999</GCIMShmKey>

  <Channels>
    <Priority>8</Priority>
    <Address>100.1.4.1</Address>
    <Address>100.1.3.1</Address>
    <SendSize>1024000</SendSize>
    <RecvSize>1024000</RecvSize>
    <Desc>dmsd and transd</Desc>
  </Channels>

  <Channels>
    <Priority>8</Priority>
    <Address>100.1.3.1</Address>
    <Address>100.1.2.1</Address>
    <SendSize>1024000</SendSize>
    <RecvSize>1024000</RecvSize>
    <Desc>transd and operd</Desc>
  </Channels>


  <Channels>
    <Priority>8</Priority>
    <Address>100.1.2.1</Address>
    <Address>100.1.1.1</Address>
    <SendSize>1024000</SendSize>
    <RecvSize>1024000</RecvSize>
    <Desc>operd and demo</Desc>
  </Channels>

</TbusGCIM>

