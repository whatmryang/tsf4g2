  <GCIMShmKey>2681</GCIMShmKey>

  <Channels>
    <Priority>8</Priority>
    <Address>2.6.8.1</Address>
    <Address>2.6.8.2</Address>
    <SendSize>102400</SendSize>
    <RecvSize>102400</RecvSize>
    <Desc>tormsvr and clt</Desc>
  </Channels>


</TbusGCIM>

